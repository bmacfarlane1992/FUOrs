path = "/san/stellar3/dstamatellos/ea/3/DE05.du.0"
#
de05_low = 444
de05_high = 1544
file_res = 10
#
de05_n = (de05_high - de05_low) / file_res
#
for i in range(0, de05_n):
	point_n = (i * file_res) + de05_low
	if (point_n < 1000):
		point_file = point_file+path+'0'+str(point_n)+'.gz '
	if (point_n > 1000):
		point_file = point_file+path+str(point_n)+'.gz '
#
point_file = "scp bmacfarlane@stargate.uclan.ac.uk:'"+point_file+"' ."
